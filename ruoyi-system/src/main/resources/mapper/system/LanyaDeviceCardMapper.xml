            <if test="updateTime != null">#{updateTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateLanyaDeviceCard" parameterType="LanyaDeviceCard">
        update device_card
        <trim prefix="SET" suffixOverrides=",">
            <if test="cardId != null">card_id = #{cardId},</if>
            <if test="cardType != null">card_type = #{cardType},</if>
            <if test="cardModel != null">card_model = #{cardModel},</if>
            <if test="icCardId != null">ic_card_id = #{icCardId},</if>
            <if test="cardPower != null">card_power = #{cardPower},</if>
            <if test="cardStatus != null">card_status = #{cardStatus},</if>
            <if test="cardVersion != null">card_version = #{cardVersion},</if>
            <if test="cardFreq != null">card_freq = #{cardFreq},</if>
            <if test="statusTime != null">status_time = #{statusTime},</if>
            <if test="cardEnable != null and cardEnable != ''">card_enable = #{cardEnable},</if>
            <if test="useStatus != null and useStatus != ''">use_status = #{useStatus},</if>
