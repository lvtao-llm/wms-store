    <sql id="selectLanyaDeviceCardVo">
        select id, card_id, card_type, card_model, ic_card_id, card_power, card_status, card_version, card_freq, status_time, card_enable, use_status, card_transfer, heart_time, create_time, create_by, update_time, update_by, remark from device_card
    </sql>

    <select id="selectLanyaDeviceCardList" parameterType="LanyaDeviceCard" resultMap="LanyaDeviceCardResult">
        <include refid="selectLanyaDeviceCardVo"/>
        <where>  
            <if test="cardId != null "> and card_id = #{cardId}</if>
            <if test="cardType != null  and cardType != ''"> and card_type = #{cardType}</if>
            <if test="cardModel != null  and cardModel != ''"> and card_model = #{cardModel}</if>
            <if test="icCardId != null  and icCardId != ''"> and ic_card_id = #{icCardId}</if>
            <if test="cardPower != null "> and card_power = #{cardPower}</if>
            <if test="cardStatus != null "> and card_status = #{cardStatus}</if>
            <if test="cardVersion != null "> and card_version = #{cardVersion}</if>
            <if test="cardFreq != null "> and card_freq = #{cardFreq}</if>
            <if test="statusTime != null "> and status_time = #{statusTime}</if>
            <if test="cardEnable != null  and cardEnable != ''"> and card_enable = #{cardEnable}</if>
            <if test="useStatus != null  and useStatus != ''"> and use_status = #{useStatus}</if>
            <if test="cardTransfer != null "> and card_transfer = #{cardTransfer}</if>
            <if test="heartTime != null "> and heart_time = #{heartTime}</if>
        </where>
    </select>
    
    <select id="selectLanyaDeviceCardById" parameterType="Long" resultMap="LanyaDeviceCardResult">
        <include refid="selectLanyaDeviceCardVo"/>
        where id = #{id}
    </select>

    <insert id="insertLanyaDeviceCard" parameterType="LanyaDeviceCard" useGeneratedKeys="true" keyProperty="id">
        insert into device_card
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="cardId != null">card_id,</if>
            <if test="cardType != null">card_type,</if>
            <if test="cardModel != null">card_model,</if>
            <if test="icCardId != null">ic_card_id,</if>
            <if test="cardPower != null">card_power,</if>
            <if test="cardStatus != null">card_status,</if>
            <if test="cardVersion != null">card_version,</if>
            <if test="cardFreq != null">card_freq,</if>
            <if test="statusTime != null">status_time,</if>
            <if test="cardEnable != null and cardEnable != ''">card_enable,</if>
            <if test="useStatus != null and useStatus != ''">use_status,</if>
            <if test="cardTransfer != null">card_transfer,</if>
            <if test="heartTime != null">heart_time,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="cardId != null">#{cardId},</if>
            <if test="cardType != null">#{cardType},</if>
            <if test="cardModel != null">#{cardModel},</if>
            <if test="icCardId != null">#{icCardId},</if>
            <if test="cardPower != null">#{cardPower},</if>
            <if test="cardStatus != null">#{cardStatus},</if>
            <if test="cardVersion != null">#{cardVersion},</if>
