<mapper namespace="com.ruoyi.system.mapper.LanyaDeviceCardMapper">
    
    <resultMap type="LanyaDeviceCard" id="LanyaDeviceCardResult">
        <result property="id"    column="id"    />
        <result property="cardId"    column="card_id"    />
        <result property="cardType"    column="card_type"    />
        <result property="cardModel"    column="card_model"    />
        <result property="icCardId"    column="ic_card_id"    />
        <result property="cardPower"    column="card_power"    />
        <result property="cardStatus"    column="card_status"    />
        <result property="cardVersion"    column="card_version"    />
        <result property="cardFreq"    column="card_freq"    />
        <result property="statusTime"    column="status_time"    />
        <result property="cardEnable"    column="card_enable"    />
        <result property="useStatus"    column="use_status"    />
