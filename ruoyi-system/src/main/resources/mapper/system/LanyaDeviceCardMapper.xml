            <if test="statusTime != null">#{statusTime},</if>
            <if test="cardEnable != null and cardEnable != ''">#{cardEnable},</if>
            <if test="useStatus != null and useStatus != ''">#{useStatus},</if>
            <if test="cardTransfer != null">#{cardTransfer},</if>
            <if test="heartTime != null">#{heartTime},</if>
            <if test="createTime != null">#{createTime},</if>
