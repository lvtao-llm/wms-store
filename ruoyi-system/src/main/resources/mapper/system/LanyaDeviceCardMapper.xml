            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteLanyaDeviceCardById" parameterType="Long">
        delete from device_card where id = #{id}
    </delete>

    <delete id="deleteLanyaDeviceCardByIds" parameterType="String">
        delete from device_card where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>