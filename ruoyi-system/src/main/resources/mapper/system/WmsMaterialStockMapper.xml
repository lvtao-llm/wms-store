    <sql id="selectWmsMaterialStockVo">
        select inventory_id, wzbm, actual_weight, status, remark, wzmc, jldw, warehouse, book_weight, pzmc, variety_name, wzlb, reserved_weight, available_weight, squadron, team, company from wms_material_stock
    </sql>

    <select id="selectWmsMaterialStockList" parameterType="WmsMaterialStock" resultMap="WmsMaterialStockResult">
        <include refid="selectWmsMaterialStockVo"/>
        <where>
            <if test="inventoryId != null  and inventoryId != ''"> and inventory_id = #{inventoryId}</if>
            <if test="wzbm != null  and wzbm != ''"> and wzbm = #{wzbm}</if>
            <if test="actualWeight != null "> and actual_weight = #{actualWeight}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="wzmc != null  and wzmc != ''"> and wzmc like concat('%', #{wzmc}, '%')</if>
            <if test="jldw != null  and jldw != ''"> and jldw = #{jldw}</if>
            <if test="warehouse != null  and warehouse != ''"> and warehouse = #{warehouse}</if>
            <if test="bookWeight != null "> and book_weight = #{bookWeight}</if>
            <if test="varietyCode != null  and varietyCode != ''"> and variety_code = #{varietyCode}</if>
            <if test="pzmc != null  and pzmc != ''"> and pzmc like concat('%', #{pzmc}, '%')</if>
            <if test="wzlb != null  and wzlb != ''"> and wzlb = #{wzlb}</if>
            <if test="reservedWeight != null "> and reserved_weight = #{reservedWeight}</if>
            <if test="availableWeight != null "> and available_weight = #{availableWeight}</if>
            <if test="squadron != null  and squadron != ''"> and squadron = #{squadron}</if>
            <if test="team != null  and team != ''"> and team = #{team}</if>
            <if test="company != null  and company != ''"> and company = #{company}</if>
        </where>
    </select>

    <select id="selectWmsMaterialStockByInventoryId" parameterType="String" resultMap="WmsMaterialStockResult">
        <include refid="selectWmsMaterialStockVo"/>
        where inventory_id = #{inventoryId}
    </select>

    <insert id="insertWmsMaterialStock" parameterType="WmsMaterialStock">
        insert into wms_material_stock
        <trim prefix="(" suffix=")" suffixOverrides=",">
