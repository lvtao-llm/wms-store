    <update id="updateWmsMaterialStock" parameterType="WmsMaterialStock">
        update wms_material_stock
        <trim prefix="SET" suffixOverrides=",">
            <if test="wzbm != null">wzbm = #{wzbm},</if>
            <if test="actualWeight != null">actual_weight = #{actualWeight},</if>
            <if test="status != null">status = #{status},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="wzmc != null">wzmc = #{wzmc},</if>
            <if test="jldw != null">jldw = #{jldw},</if>
