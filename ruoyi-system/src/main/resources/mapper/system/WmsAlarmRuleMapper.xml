            <if test="imNoticeUsers != null">#{imNoticeUsers},</if>
            <if test="sysNoticeUsers != null">#{sysNoticeUsers},</if>
         </trim>
    </insert>

    <update id="updateWmsAlarmRule" parameterType="WmsAlarmRule">
        update wms_alarm_rule
        <trim prefix="SET" suffixOverrides=",">
            <if test="alarmRuleName != null">alarm_rule_name = #{alarmRuleName},</if>
            <if test="alarmRuleTimeThreshold != null">alarm_rule_time_threshold = #{alarmRuleTimeThreshold},</if>
            <if test="alarmRuleDistThreshold != null">alarm_rule_dist_threshold = #{alarmRuleDistThreshold},</if>
            <if test="alarmRuleTargetAreaCode != null">alarm_rule_target_area_code = #{alarmRuleTargetAreaCode},</if>
            <if test="alarmRuleType != null">alarm_rule_type = #{alarmRuleType},</if>
            alarm_rule_effective_period_s = #{alarmRuleEffectivePeriodS},
            alarm_rule_effective_period_e = #{alarmRuleEffectivePeriodE},
            <if test="alarmRuleEnabled != null">alarm_rule_enabled = #{alarmRuleEnabled},</if>
            <if test="alarmRuleRelatedPeople != null">alarm_rule_related_people = #{alarmRuleRelatedPeople},</if>
            <if test="alarmRuleRelatedDept != null">alarm_rule_related_dept = #{alarmRuleRelatedDept},</if>
            <if test="alarmRuleNoticeType != null">alarm_rule_notice_type = #{alarmRuleNoticeType},</if>
            <if test="maxPeopleCount != null">max_people_count = #{maxPeopleCount},</if>
            <if test="alarmType != null">alarm_type = #{alarmType},</if>
            <if test="smsNoticeUsers != null">sms_notice_users = #{smsNoticeUsers},</if>
            <if test="imNoticeUsers != null">im_notice_users = #{imNoticeUsers},</if>
            <if test="sysNoticeUsers != null">sys_notice_users = #{sysNoticeUsers},</if>
        </trim>
