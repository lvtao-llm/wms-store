    <delete id="deleteWmsAlarmRuleByAlarmRuleIds" parameterType="String">
        delete from wms_alarm_rule where alarm_rule_id in 
        <foreach item="alarmRuleId" collection="array" open="(" separator="," close=")">
            #{alarmRuleId}
        </foreach>
    </delete>
