        <result property="imNoticeUsers" column="im_notice_users"/>
        <result property="sysNoticeUsers" column="sys_notice_users"/>
    </resultMap>

    <sql id="selectWmsAlarmRuleVo">
        select alarm_rule_id,
               alarm_rule_name,
               alarm_rule_time_threshold,
               alarm_rule_dist_threshold,
               max_people_count,
               alarm_rule_target_area_code,
               alarm_rule_type,
               alarm_rule_effective_period_s,
               alarm_rule_effective_period_e,
               alarm_rule_enabled,
               alarm_rule_related_people,
               alarm_rule_related_dept,
               alarm_rule_notice_type,
               alarm_type,
               sms_notice_users,
               im_notice_users,
               sys_notice_users
        from wms_alarm_rule
    </sql>

    <select id="selectWmsAlarmRuleList" parameterType="WmsAlarmRule" resultMap="WmsAlarmRuleResult">
        <include refid="selectWmsAlarmRuleVo"/>
        <where>
            <if test="alarmRuleId != null ">and alarm_rule_id = #{alarmRuleId}</if>
            <if test="alarmRuleName != null  and alarmRuleName != ''">and alarm_rule_name like concat('%',
                #{alarmRuleName}, '%')
            </if>
            <if test="alarmRuleTimeThreshold != null  and alarmRuleTimeThreshold != ''">and alarm_rule_time_threshold =
                #{alarmRuleTimeThreshold}
            </if>
            <if test="alarmRuleDistThreshold != null  and alarmRuleDistThreshold != ''">and alarm_rule_dist_threshold =
                #{alarmRuleDistThreshold}
            </if>
            <if test="alarmRuleTargetAreaCode != null  and alarmRuleTargetAreaCode != ''">and
                alarm_rule_target_area_code = #{alarmRuleTargetAreaCode}
            </if>
            <if test="alarmRuleType != null  and alarmRuleType != ''">and alarm_rule_type = #{alarmRuleType}</if>
            <if test="alarmRuleEffectivePeriodS != null  and alarmRuleEffectivePeriodS != ''">and
                alarm_rule_effective_period_s = #{alarmRuleEffectivePeriodS}
            </if>
            <if test="alarmRuleEffectivePeriodE != null  and alarmRuleEffectivePeriodE != ''">and
                alarm_rule_effective_period_e = #{alarmRuleEffectivePeriodE}
            </if>
            <if test="alarmRuleEnabled != null ">and alarm_rule_enabled = #{alarmRuleEnabled}</if>
            <if test="alarmRuleRelatedPeople != null  and alarmRuleRelatedPeople != ''">and alarm_rule_related_people =
                #{alarmRuleRelatedPeople}
            </if>
            <if test="alarmRuleRelatedDept != null  and alarmRuleRelatedDept != ''">and alarm_rule_related_dept =
                #{alarmRuleRelatedDept}
            </if>
            <if test="alarmRuleNoticeType != null  and alarmRuleNoticeType != ''">and alarm_rule_notice_type =
                #{alarmRuleNoticeType}
            </if>
            <if test="maxPeopleCount != null  and alarmRuleNoticeType != ''">and max_people_count = #{maxPeopleCount}
            </if>
            <if test="alarmType != null  and alarmType != ''">and alarm_type = #{alarmType}</if>
            <if test="smsNoticeUsers != null  and smsNoticeUsers != ''">and sms_notice_users = #{smsNoticeUsers}</if>
            <if test="imNoticeUsers != null  and imNoticeUsers != ''">and im_notice_users = #{imNoticeUsers}</if>
            <if test="sysNoticeUsers != null  and sysNoticeUsers != ''">and sys_notice_users = #{sysNoticeUsers}</if>
        </where>
    </select>
    
    <select id="selectWmsAlarmRuleByAlarmRuleId" parameterType="Long" resultMap="WmsAlarmRuleResult">
        <include refid="selectWmsAlarmRuleVo"/>
        where alarm_rule_id = #{alarmRuleId}
    </select>

    <insert id="insertWmsAlarmRule" parameterType="WmsAlarmRule">
        insert into wms_alarm_rule
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="alarmRuleId != null">alarm_rule_id,</if>
            <if test="alarmRuleName != null">alarm_rule_name,</if>
            <if test="alarmRuleTimeThreshold != null">alarm_rule_time_threshold,</if>
            <if test="alarmRuleDistThreshold != null">alarm_rule_dist_threshold,</if>
            <if test="alarmRuleTargetAreaCode != null">alarm_rule_target_area_code,</if>
            <if test="alarmRuleType != null">alarm_rule_type,</if>
            <if test="alarmRuleEffectivePeriodS != null">alarm_rule_effective_period_s,</if>
            <if test="alarmRuleEffectivePeriodE != null">alarm_rule_effective_period_e,</if>
            <if test="alarmRuleEnabled != null">alarm_rule_enabled,</if>
            <if test="alarmRuleRelatedPeople != null">alarm_rule_related_people,</if>
            <if test="alarmRuleRelatedDept != null">alarm_rule_related_dept,</if>
            <if test="alarmRuleNoticeType != null">alarm_rule_notice_type,</if>
            <if test="maxPeopleCount != null">max_people_count,</if>
            <if test="alarmType != null">alarm_type,</if>
            <if test="smsNoticeUsers != null">sms_notice_users,</if>
            <if test="imNoticeUsers != null">im_notice_users,</if>
            <if test="sysNoticeUsers != null">sys_notice_users,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="alarmRuleId != null">#{alarmRuleId},</if>
            <if test="alarmRuleName != null">#{alarmRuleName},</if>
            <if test="alarmRuleTimeThreshold != null">#{alarmRuleTimeThreshold},</if>
            <if test="alarmRuleDistThreshold != null">#{alarmRuleDistThreshold},</if>
            <if test="alarmRuleTargetAreaCode != null">#{alarmRuleTargetAreaCode},</if>
            <if test="alarmRuleType != null">#{alarmRuleType},</if>
            <if test="alarmRuleEffectivePeriodS != null">#{alarmRuleEffectivePeriodS},</if>
            <if test="alarmRuleEffectivePeriodE != null">#{alarmRuleEffectivePeriodE},</if>
            <if test="alarmRuleEnabled != null">#{alarmRuleEnabled},</if>
            <if test="alarmRuleRelatedPeople != null">#{alarmRuleRelatedPeople},</if>
