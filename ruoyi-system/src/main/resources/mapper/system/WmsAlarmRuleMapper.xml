        <result property="alarmRuleRelatedPeople"    column="alarm_rule_related_people"    />
        <result property="alarmRuleRelatedDept"    column="alarm_rule_related_dept"    />
        <result property="alarmRuleNoticeType"    column="alarm_rule_notice_type"    />
